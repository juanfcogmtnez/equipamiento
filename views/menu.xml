<?xml version="1.0" encoding="utf-8" ?>

<odoo>
	<record id="action_open_equipamiento" model="ir.actions.act_window">
		<field name="name">Equipamiento</field>
		<field name="res_model">equipamiento</field>
		<field name="view_mode">tree,form</field>
	</record>
	<record id="action_open_ull" model="ir.actions.act_window">
		<field name="name">Ull</field>
		<field name="res_model">ull</field>
		<field name="view_mode">tree,form</field>
	</record>
	<menuitem id="menu_principal" name="Equipamiento"/>
	<menuitem id="menu_horizontal" name="Equipos" parent="menu_principal"/>
	<menuitem id="menu_horizontal_ull" name="ULLs" parent="menu_principal"/>
	<menuitem id="menu_vertical_equipamiento" name="Equipo" parent="menu_horizontal" action="action_open_equipamiento"/>
	<menuitem id="menu_vertical_ull" name="Ulls" parent="menu_horizontal_ull" action="action_open_ull"/>
</odoo>
